<%= form_for(@news) do |f| %>
<%= render 'shared/error_messages', :object => f.object %>
<div class="field">
  <%= f.label :title %>
  <br />
  <%= f.text_field :title %>
</div>
<div class="field">
  <%= f.label :content %>
  <br />
  <%= f.text_area :content %>
</div>
<div class="actions">
  <%= f.submit %>
</div>
<% end %>
